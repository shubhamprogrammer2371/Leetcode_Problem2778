int sumOfSquares(int* nums, int numsSize){
    int res = 0;
    for(int i = 1 ; i <= numsSize ; i++)
        res = !(numsSize % i) ? res + nums[i-1]*nums[i-1] : res;
    return res;
}


------------------------------------------------------------------------


int sumOfSquares(int* nums, int numsSize){
    int res = 0;
    for(int i = 1 ; i <= numsSize ; i++)
        res = !(numsSize % i) ? res + pow(nums[i-1],2) : res;
    return res;
}
